<h2>Song Title: <%= @song.title %></h2>
<% if @song.released %>
    <p>Year: <%= @song.release_year %></p>
<% end %>
<p>Artist: <%= @song.artist_name %></p>
<p><%= @song.genre %></p>
<%= link_to "edit", edit_song_path(@song) %>
<%= link_to "delete", @song, method: :delete, data: { confirm: "Do you want to delete this song?" } %>
