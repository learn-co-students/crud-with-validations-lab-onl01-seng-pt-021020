<h1>Songs</h1>

# <div><%= link_to 'Create Song', new_song_path %></div>
# <br />
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Released</th>
            <th>Release Year</th>
            <th>Artist Name</th>
            <th>Genre</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td><br>
                <td><%= song.released %></td><br>
                <td><%= song.release_year %></td><br>
                <td><%= song.artist_name %></td><br>
                <td><%= song.genre %></td><br>
            </tr>
        <% end %>
    </tbody>
</table> 